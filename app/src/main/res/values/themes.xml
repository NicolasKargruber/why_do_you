<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TimeKeeper" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/gray_900</item>
        <item name="colorPrimaryVariant">@color/blue_gray_500</item>
        <item name="colorPrimaryDark">@color/gray_500</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/purple_custom</item>
        <item name="colorSecondaryVariant">@color/purple_custom_dark</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
        <item name="colorOutline">#1E000000</item>
        <item name="errorIconTint">@color/onError</item>

    </style>

    <style name="Theme.TimeKeeper.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TimeKeeper.AppBarOverlay" parent="ThemeOverlay.AppCompat.Light" >
    </style>

    <style name="Theme.TimeKeeper.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" >
    </style>
</resources>